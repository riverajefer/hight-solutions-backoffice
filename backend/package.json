{
  "name": "nestjs-auth-rbac",
  "version": "1.0.0",
  "description": "NestJS API with JWT authentication and RBAC (Role-Based Access Control)",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:staging": "NODE_ENV=staging node dist/main",
    "start:prod": "NODE_ENV=production node dist/main",
    "env:info": "ts-node scripts/env-info.ts",
    "prisma:reset": "prisma migrate reset --force && npm run prisma:seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run prisma:seed",
    "db:setup": "prisma migrate dev --name init && npm run prisma:seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "nestjs",
    "prisma",
    "authentication",
    "jwt",
    "rbac",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.989.0",
    "@aws-sdk/s3-request-presigner": "^3.989.0",
    "@explita/prisma-audit-log": "^0.2.1",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.2.0",
    "@prisma/config": "^7.2.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cpx": "^1.5.0",
    "dotenv": "^17.2.3",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.8",
    "@types/node-fetch": "^2.6.13",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
